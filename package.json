{
  "name": "dismoi",
  "version": "0.2.0",
  "description": "dependency injection for javascript projects",
  "main": "src/index.cjs",
  "types": "src/index.d.ts",
  "scripts": {
    "test:unit": "pta",
    "test:types": "tsc src/test.ts --noEmit",
    "test": "npm run test:unit && npm run test:types",
    "build": "rollup -c rollup.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorenzofox3/dismoi.git"
  },
  "keywords": [
    "di",
    "dependency",
    "injection",
    "provide",
    "dependency injection",
    "ioc"
  ],
  "files": [
    "src/index.js",
    "src/index.d.ts",
    "src/provider.js",
    "src/index.cjs"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./src/index.js",
        "types": "./src/index.d.ts"
      },
      "require": {
        "default": "./src/index.cjs"
      }
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "author": "Laurent RENARD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lorenzofox3/dismoi/issues"
  },
  "homepage": "https://github.com/lorenzofox3/dismoi#readme",
  "devDependencies": {
    "prettier": "^2.8.7",
    "pta": "^1.2.0",
    "rollup": "^3.21.0",
    "typescript": "^5.0.4"
  }
}
